  "scripts": {
    "start": "npm run nodemon",
    "node": "node src",
    "nodemon": "npx nodemon src/index.mjs",
    "dashboard": "nodejs-dashboard node src/index.mjs",
    "trace": "npx nodemon --inspect --trace_gc src/index.mjs",
    "limit": "npx nodemon --inspect  --max_old_space_size=400 src/index.mjs",
    "inspect": "npx nodemon --inspect src",
    "ssldevloc": "set NODE_PATH=&&set HTTPS=true&&set NODE_ENV=development&&set SERVERTARGET=localhost&&npm run nodemon",
    "ssldevvps": "set NODE_PATH=&&set HTTPS=true&&set NODE_ENV=development&&set SERVERTARGET=vps&&npm run nodemon",
    "sslprod": "set NODE_PATH=&&set HTTPS=true&&set NODE_ENV=production&&set SERVERTARGET=vps&&npm run node",
    "nossldevloc": "set NODE_PATH=&&set HTTPS=false&&set NODE_ENV=development&&set SERVERTARGET=localhost&&npm run nodemon",
    "nossldevvps": "set NODE_PATH=&&set HTTPS=false&&set NODE_ENV=development&&set SERVERTARGET=vps&&npm run nodemon",
    "nosslprod": "set NODE_PATH=&&set HTTPS=false&&set NODE_ENV=production&&set SERVERTARGET=vps&&npm start",
    "ssh": "ssh -p 22 root@208.167.253.67",
    "reboot": "ssh -p 22 root@208.167.253.67 reboot",
    "pm2list": "ssh -p 22 root@208.167.253.67 pm2 list",
    "pm2logs": "ssh -p 22 root@208.167.253.67 pm2 logs",
    "pm2monit": "ssh -p 22 root@208.167.253.67 pm2 monit",
    "pm2runnodemon": "ssh -p 22 root@208.167.253.67 cd server;pm2 start npm --no-automation -- start;pm2 save",
    "pm2runnode": "ssh -p 22 root@208.167.253.67 cd server;pm2 start npm --no-automation -- run node;pm2 save",
    "pm2stop": "ssh -p 22 root@208.167.253.67 pm2 stop",
    "pm2restart": "ssh -p 22 root@208.167.253.67 pm2 restart",
    "pm2delete": "ssh -p 22 root@208.167.253.67 pm2 delete all",
    "pm2hotreload": "ssh -p 22 root@208.167.253.67 pm2 reload all",
    "log": "ssh -p 22 root@208.167.253.67 cat /root/.pm2/logs/npm-out.log",
    "ufwstatus": "ssh -p 22 root@208.167.253.67 sudo ufw status",
    "ufwopen8443": "ssh -p 22 root@208.167.253.67 sudo ufw allow 8443",
    "chmod600": "ssh -p 22 root@208.167.253.67 chmod -R 0600 0600.database;chmod -R 0600 0600.sslcert",
    "deploy": "scp -P 22 -r src root@208.167.253.67:./server/",
    "scp": "scp -P 22 -r src root@208.167.253.67:./server/",
    "budossl": "npx budo src --live --ssl --cors",
    "nginxwinstart": "start nginx; tasklist /fi \"imagename eq nginx.exe\"",
    "nginxwinreload": "nginx -s reload; tasklist /fi \"imagename eq nginx.exe\"",
    "nginxwinstop": "nginx -s stop; tasklist /fi \"imagename eq nginx.exe\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
